import javax.swing.JTextField;

public class PitcherBean {
	private int no;
	private String team;
	private String player; 
	private int num;
	private String name;
	private double era;
	private int strike;
	
	//생성자
	public PitcherBean() {
		super();
	}
	
	public PitcherBean(int no, String team, String player, int num, String name, double era, int strike) {
		super();
		this.no = no;
		this.team = team;
		this.player = player;
		this.num = num;
		this.name = name;
		this.era = era;
		this.strike = strike;
	}

	//setter
	public void setNo(int no) {
		this.no = no;
	}
	public void setTeam(String team) {
		this.team = team;
	}
	public void setPlayer(String player) {
		this.player = player;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setEra(double era) {
		this.era = era;
	}
	public void setStrike(int strike) {
		this.strike = strike;
	}
	
	
	//getter
	public int getNo() {
		return no;
	}
	public String getTeam() {
		return team;
	}
	public String getPlayer() {
		return player;
	}
	public int getNum() {
		return num;
	}
	public String getName() {
		return name;
	}
	public double getEra() {
		return era;
	}
	public int getStrike() {
		return strike;
	}
	
}
